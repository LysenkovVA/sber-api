{
  "name": "sber-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:db:client:generate": "dotenv -e .env.development -- npm exec prisma generate --schema",
    "dev:db:reset": "dotenv -e .env.development -- npx prisma migrate reset --force --skip-seed",
    "dev:db:push": "npm run dev:db:client:generate && dotenv -e .env.development -- npm exec prisma db push",
    "dev:db:seed": "dotenv -e .env.development -- npx prisma db seed",
    "dev:db:init": "npm run dev:db:client:generate && npm run dev:db:reset && npm run dev:db:push && npm run dev:db:seed",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "schema": "./src/database/prisma",
    "seed": "ts-node ./src/database/seed.ts"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.0.2",
    "@prisma/client": "^6.7.0",
    "@reduxjs/toolkit": "^2.7.0",
    "antd": "^5.24.9",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "next": "15.3.1",
    "pg": "^8.15.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^6.0.0",
    "react-redux": "^9.2.0",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20"
  }
}
